model User {
  pk       Int     @id @default(autoincrement())
  id       String  @unique @default(uuid())
  username String  @unique
  email    String? @unique
  password String?

  credential  Credential?
  files       File[]
  folders     Folder[]
  FileSharers FileSharers[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
