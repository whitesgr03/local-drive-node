model User {
  pk       Int     @id @default(autoincrement())
  id       String  @unique @default(uuid())
  username String  @unique
  email    String? @unique
  password String?

  credential Credential?
  files      File[]
  folders    Folder[]
  shared     Sharing[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
